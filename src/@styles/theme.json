{
  "font": {
    "primary": "-apple-system, 'Helvetica Neue', 'Segoe UI', Roboto, Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
    "secondary": "'Petrona', serif",
    "size": {
      "xs": "13px",
      "sm": "14px",
      "md": "15px",
      "lg": "16px",
      "xl": "17px"
    },
    "variant": {
      "h1": {
        "size": "6.25rem",
        "lineHeight": "0.9"
      },
      "h2": {
        "size": "4.5rem",
        "lineHeight": "1.0"
      },
      "h3": {
        "size": "2.75rem",
        "lineHeight": "1.1"
      },
      "h4": {
        "size": "2.5rem",
        "lineHeight": "1.2"
      },
      "h5": {
        "size": "2.25rem",
        "lineHeight": "1.3"
      },
      "h6": {
        "size": "1.875rem",
        "lineHeight": "1.4"
      },
      "h7": {
        "size": "1.5rem",
        "lineHeight": "1.5"
      },
      "h8": {
        "size": "1.125rem",
        "lineHeight": "1.625"
      },
      "default": {
        "size": "1rem",
        "lineHeight": "1.5"
      },
      "small": {
        "size": "0.9rem",
        "lineHeight": "1.6"
      },
      "smaller": {
        "size": "0.8rem",
        "lineHeight": "1.7"
      },
      "smallest": {
        "size": "0.7rem",
        "lineHeight": "1.8",
        "weight": null
      }
    }
  },
  "grid": {
    "columns": 12,
    "size": "1fr",
    "columnGap": "Max(4vw, Min(96px, 5vw))",
    "rowGap": "0"
  },
  "maxWidth": {
    "xs": "calc(100% - var(--grid-column-gap) * 2)",
    "sm": "calc(100% - var(--grid-column-gap) * 2)",
    "md": "Min(928px, calc(100% - var(--grid-column-gap) * 2))",
    "lg": "Min(1344px, calc(100% - var(--grid-column-gap) * 4))",
    "xl": "Min(1204px, calc(100% - var(--grid-column-gap) * 6))"
  },
  "mediaQuery": {
    "xs": 0,
    "sm": 768,
    "md": 1024,
    "lg": 1440,
    "xl": 1600
  },
  "button": {
    "borderRadius": "0"
  },
  "card": {
    "shadow": "0 20px 20px -10px rgba(0,0,0,0.2)"
  },
  "border": {
    "radius": "0.5em"
  },
  "shadow": {
    "lg": "0 1rem 4rem -0.5rem rgba(0, 0, 0, 0.9)"
  },
  "backdrop": {
    "opacity": 0.5
  },
  "color": {
    "neutral": {
      "100": "#FFFFFF",
      "200": "#DFDFE2",
      "300": "#BFBFC5",
      "400": "#9F9FA8",
      "500": "#7E7E8B",
      "600": "#61616B",
      "700": "#44444B",
      "800": "#27272B",
      "900": "#09090A"
    },
    "primary": {
      "100": "#DDDAFD",
      "200": "#BBB6FC",
      "300": "#9790F7",
      "400": "#7A72EF",
      "500": "#4F46E5",
      "600": "#3A33C4",
      "700": "#2923A4",
      "800": "#1A1684",
      "900": "#100D6D"
    },
    "secondary": {
